<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:customRenderer="clr-namespace:CustomRendererXF.CustomRenderer;assembly=CustomRendererXF"
             x:Class="CustomRendererXF.Views.CustomEntryPage">
    <ContentPage.Content>
        <StackLayout Padding="30" VerticalOptions="CenterAndExpand">
            
            <Entry Text="Default Android Entry"/>

            <customRenderer:RoundedEntry Text="Custom Entry"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>